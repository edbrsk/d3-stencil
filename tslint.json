{
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-shadowed-variable": [
      true,
      {
        "class": true,
        "enum": true,
        "function": true,
        "interface": false,
        "namespace": true,
        "typeAlias": false,
        "typeParameter": false
      }
    ],
    "no-submodule-imports": [
      true,
      "@stencil/core/testing",
      "@d3-stencil/decorators",
      "@d3-stencil/interfaces",
      "@d3-stencil/shared",
      "@d3-stencil/utils"
    ],
    "no-implicit-dependencies": false
  },
  "extends": ["tslint:latest", "tslint-config-prettier"]
}
